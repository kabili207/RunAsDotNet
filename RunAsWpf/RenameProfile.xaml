<Window x:Class="RunAsDotNet.RenameProfile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Rename Profile" Height="145" Width="300" WindowStartupLocation="CenterOwner" ResizeMode="NoResize" Icon="/RunAsDotNet;component/runas.ico">
    <Grid>
		<TextBox Height="23" Margin="12,40,12,0" Name="txtProfileName" VerticalAlignment="Top" />
		<Label Content="Enter a new name for this profile" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1" VerticalAlignment="Top" />
		<Button Content="OK" Height="23" HorizontalAlignment="Right" Margin="0,0,97,12" Name="btnOK" VerticalAlignment="Bottom" Width="75" Click="btnOK_Click" />
		<Button Content="Cancel" HorizontalAlignment="Right" Margin="0,0,12,12" Name="btnCancel" Width="75" Height="23" VerticalAlignment="Bottom" Click="btnCancel_Click" />
	</Grid>
</Window>
